<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="confirmPaymentDetailsOneVM" type="com.pasindusapplication.app.modules.confirmpaymentdetailsone.data.viewmodel.ConfirmPaymentDetailsOneVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStack"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <FrameLayout android:id="@+id/frameConfirmPayment"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStylewhite_A700"
            >
            <FrameLayout android:id="@+id/frameStackrileyjzzkyohaone"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_579pxv"
                android:layout_marginBottom="@dimen/_10pxv" android:layout_width="match_parent"
                >
                <ImageView android:id="@+id/imageRileyJZzkyOHaOne"
                    android:layout_gravity="start"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_rileyjzzkyoha1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rileyjzzkyoha1"/>
                <LinearLayout android:id="@+id/linearColumnlanguage"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_40pxh"
                    android:layout_marginVertical="@dimen/_40pxv"
                    android:layout_width="match_parent" android:orientation="vertical"
                    >
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLanguage"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{confirmPaymentDetailsOneVM.confirmPaymentDetailsOneModel.txtLanguage}"
                        style="@style/txtInterbold16" tools:text="@string/msg_enter_your_car"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRow"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_54pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout android:id="@+id/linearRowrefresh"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageRefresh"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_marginBottom="@dimen/_9pxv"
                                android:layout_marginTop="@dimen/_7pxv"
                                android:layout_width="@dimen/_20pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_refresh"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_refresh"/>
                            <ImageView
                                android:id="@+id/imageDownloadOne"
                                android:layout_height="@dimen/_36pxh"
                                android:layout_marginStart="@dimen/_20pxh"
                                android:layout_width="@dimen/_36pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_download1"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_download1"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linearRowcontrast"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_7pxv"
                            android:layout_marginStart="@dimen/_77pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_weight="0.5"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageContrast"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_width="@dimen/_20pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_contrast"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_contrast"/>
                            <ImageView
                                android:id="@+id/imageKisspngmasterc"
                                android:layout_height="@dimen/_24pxh"
                                android:layout_marginStart="@dimen/_19pxh"
                                android:layout_width="@dimen/_36pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_kisspngmasterc"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_kisspngmasterc"/>
                        </LinearLayout>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCardNumber"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{confirmPaymentDetailsOneVM.confirmPaymentDetailsOneModel.txtCardNumber}"
                        style="@style/txtInterregular16" tools:text="@string/lbl_card_number"/>

                    <EditText
                        android:id="@+id/etRectangleFive"
                        style="@style/etSolidRoundedOutline"
                        android:layout_width="320dp"
                        android:layout_height="50dp"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:background="@drawable/rectangle_bg_white_a700_border_bluegray_101_radius_10"
                        android:ellipsize="end"
                        android:inputType="text"
                        android:singleLine="true"
                        android:text="@={confirmPaymentDetailsOneVM.confirmPaymentDetailsOneModel.etRectangleFiveValue}"
                        tools:ignore="Autofill"
                        android:hint="e.g 5"
                        />
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowexpiration"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_24pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtExpiration"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.68"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{confirmPaymentDetailsOneVM.confirmPaymentDetailsOneModel.txtExpiration}"
                            style="@style/txtInterregular16" tools:text="@string/lbl_expiration"

                            />
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtCCV"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_73pxh"
                            android:layout_weight="0.32"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{confirmPaymentDetailsOneVM.confirmPaymentDetailsOneModel.txtCCV}"
                            style="@style/txtInterregular16" tools:text="@string/lbl_ccv"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/linearRowrectangleeight"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">

                        <EditText
                            android:id="@+id/etRectangleEight"
                            style="@style/etSolidRoundedOutline"
                            android:layout_width="0dp"
                            android:layout_height="50dp"
                            android:layout_weight="0.5"
                            android:background="@drawable/rectangle_bg_white_a700_border_bluegray_101_radius_10"
                            android:ellipsize="end"
                            android:inputType="text"
                            android:singleLine="true"
                            android:text="@={confirmPaymentDetailsOneVM.confirmPaymentDetailsOneModel.etRectangleEightValue}"
                            tools:ignore="Autofill"

                            android:hint="hello"
                            />

                        <EditText
                            android:id="@+id/etRectangleNine"
                            style="@style/etSolidRoundedOutline"
                            android:layout_width="0dp"
                            android:layout_height="50dp"
                            android:layout_marginStart="@dimen/_54pxh"
                            android:layout_weight="0.5"
                            android:background="@drawable/rectangle_bg_white_a700_border_bluegray_101_radius_10"
                            android:ellipsize="end"
                            android:inputType="text"
                            android:singleLine="true"
                            android:text="@={confirmPaymentDetailsOneVM.confirmPaymentDetailsOneModel.etRectangleNineValue}"
                            tools:ignore="Autofill"
                            android:hint="e.g 5"
                            />
                    </LinearLayout>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnConfirm"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_44pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_11pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_11pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_confirm"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded" tools:text="@string/lbl_confirm"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnCancel"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_17pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_11pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_11pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_cancel"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded_1" tools:text="@string/lbl_cancel"/>
                </LinearLayout>
            </FrameLayout>

            <FrameLayout
                android:id="@+id/frameStackundrawplaincr"
                style="@style/groupStyleteal_900cornerRadius"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|start"
                android:layout_marginTop="@dimen/_10pxv">

                <ImageView
                    android:id="@+id/imageUndrawplaincr"
                    android:layout_width="@dimen/_161pxh"
                    android:layout_height="@dimen/_61pxh"
                    android:layout_gravity="top|start"
                    android:layout_marginStart="@dimen/_86pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_marginEnd="@dimen/_86pxh"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_undrawplaincr"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_undrawplaincr" />
            </FrameLayout>
        </FrameLayout>
    </FrameLayout>
</layout>
