<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listrefreshRowModel" type="com.pasindusapplication.app.modules.selectpaymentmethodone.data.model.ListrefreshRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowrefresh"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageRefresh"
            android:layout_height="@dimen/_20pxh"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginTop="@dimen/_5pxv"
            android:layout_width="@dimen/_20pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_refresh"
            tools:ignore="ContentDescription" tools:src="@drawable/img_refresh"/>
        <TextView android:ellipsize="end" android:id="@+id/txtCash"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginStart="@dimen/_41pxh"
            android:layout_weight="0.48" android:layout_width="0dp"
            android:singleLine="true"
            android:text="@{listrefreshRowModel.txtCash}"
            style="@style/txtInterregular20" tools:text="@string/lbl_cash"/>
        <ImageView android:id="@+id/imageVolume"
            android:layout_height="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_40pxh"
            android:layout_marginTop="@dimen/_3pxv"
            android:layout_width="@dimen/_32pxh"
            android:scaleType="fitXY" android:src="@drawable/img_volume"
            tools:ignore="ContentDescription" tools:src="@drawable/img_volume"/>
    </LinearLayout>
</layout>
